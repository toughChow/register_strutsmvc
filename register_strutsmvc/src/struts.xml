<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>


	<package name="default" namespace="/" extends="struts-default">
		
		<action name="register" class="com.qingshixun.register.RegisterAction"
			method="register">
			<result type="dispatcher">/WEB-INF/views/register.jsp</result>
		</action>
		
		<action name="regist" class="com.qingshixun.register.RegisterAction"
			method="regist">
			<result>/WEB-INF/views/success.jsp</result>
		</action>
		
	</package>
</struts>
